//1.Area Of Circle Java Program

package javatpoint;

import java.util.Scanner;

public class Student{
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		//area of circle= pi*r*r;
		
		System.out.print("Enter the radius: ");
		float radius = input.nextFloat();
		float pi = 3.14f;
		
		double area = pi*radius*radius;
		System.out.print("Enter the radius: "+area);
	}
}

//2.Area Of Triangle

package javatpoint;

import java.util.Scanner;

public class Student{
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		//area of triangle = 1/2 * base * height;
		
		System.out.print("Enter the Base: ");
		int base = input.nextInt();
		System.out.print("Enter the height: ");
		int height = input.nextInt();
		
		double area =   (base* height)/2;
		System.out.print("The area of traingle is: "+area);
	}
}


//3.Area Of Rectangle Program

package javatpoint;

import java.util.Scanner;

public class Student{
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		//Area of rectangle = length * breadth;
		System.out.print("Enter the length: ");
		int length = input.nextInt();
		System.out.print("Enter the breadth: ");
		int breadth = input.nextInt();
		
		int area = length*breadth;
		System.out.println("The area is: " + area);
	}
}

//4. Area Of Isosceles Triangle

package javatpoint;

import java.util.Scanner;

public class Student{
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		//Area of  Isosceles Triangle = (base*height)/2;
		System.out.print("Enter the base: ");
		double base = input.nextInt();
		System.out.print("Enter the height: ");
		double height = input.nextInt();
		
		double area = (base*height)/2;
		System.out.println("The area is: " + area);
	}
}

//5.Area Of Parallelogram

package javatpoint;

import java.util.Scanner;

public class Student{
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		//Area of Parallelogram = (base*height);
		System.out.print("Enter the base: ");
		double base = input.nextDouble();
		System.out.print("Enter the height: ");
		double height = input.nextDouble();
		
		double area = (base*height);
		System.out.println("The area is: " + area);
	}
}


//6. package javatpoint;

import java.util.Scanner;

public class Student{
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		//Area Of Rhombus = (d1*d2)/2;
		System.out.print("Enter the diagonal 1: ");
		double d1 = input.nextDouble();
		System.out.print("Enter the daigonal 2: ");
		double d2 = input.nextDouble();
		
		double area = (d1*d2)/2;
		System.out.println("The area is: " + area);
	}
}


//7. Area Of Equilateral Triangle

package javatpoint;

import java.util.Scanner;

public class Student{
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		//Area Of Equilateral Triangle =(Math.sqrt(3)/4)*(a*a) ;
		
		System.out.print("Enter the side of triangle: ");
		double a = input.nextDouble();
		
		
		double area = (Math.sqrt(3)/4)*(a*a);
		System.out.println("The area is: " + area);
	}
}


//8.Perimeter Of Circle

package javatpoint;

import java.util.Scanner;

public class Student{
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		//Perimeter Of Circle = 2*pi*r ;
		
		System.out.print("Enter the radius of circle: ");
		float r = input.nextFloat();
		float pi = 3.14f;
		
		
		double area = 2*pi*r;
		System.out.println("The area is: " + area);
	}
}


//9.Perimeter Of Equilateral Triangle

package javatpoint;

import java.util.Scanner;

public class Student{
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		//Perimeter Of Equilateral Triangle = 3*a ;
		
		System.out.print("Enter the side of the triangle: ");
		float a = input.nextFloat();
		
		
		double area = 3*a;
		System.out.println("The area is: " + area);
	}
}



//10. Perimeter Of Parallelogram

package javatpoint;

import java.util.Scanner;

public class Student{
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		//Perimeter Of Parallelogram = 2*(a+b)  ;
		
		System.out.print("Enter one side of the parallelogram: ");
		float a = input.nextFloat();
		System.out.print("Enter another side of the parallelogram: ");
		float b = input.nextFloat();
		
		
		double area = 2*(a+b);
		System.out.println("The area is: " + area);
	}
}


//11. Perimeter Of Rectangle

package javatpoint;

import java.util.Scanner;

public class Student{
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		//Perimeter Of Rectangle = 2*(length+width)  ;
		
		System.out.print("Enter length: ");
		float length = input.nextFloat();
		System.out.print("Enter width: ");
		float width = input.nextFloat();
		
		
		double area = 2*(length+width);
		System.out.println("The area is: " + area);
	}
}


