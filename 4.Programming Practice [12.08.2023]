//14.Volume Of Cone Java Program

package javatpoint;

import java.util.Scanner;

public class Student{
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		//Volume of Cone = 1/3(pi * r * r * h)  ;
		
		System.out.print("Enter the radius: ");
		float radius = input.nextFloat();
		System.out.print("Enter the height: ");
		float height = input.nextFloat();
		float pi = 3.14f;
		
		double area = (pi*radius*radius*height)/3;
		System.out.println("The area is: " + area);
	}
}


//15.Volume Of Traingular Prism

package javatpoint;

import java.util.Scanner;

public class Student{
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		//Volume Of Triangular Prism = (Apothem length * base length * height)/2  ;
		
		System.out.print("Enter the Apothem length: ");
		float a = input.nextFloat();
		System.out.print("Enter the Base: ");
		float b = input.nextFloat();
		System.out.print("Enter the height: ");
		float h = input.nextFloat();
		
		double area = (a*b*h)/2;
		System.out.println("The area is: " + area);
	}
}


//16.Volume Of Cylinder

package javatpoint;

import java.util.Scanner;

public class Student{
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		//Volume Of Cylinder = pi * r*r* h  ;
		
		System.out.print("Enter the radius: ");
		float radius = input.nextFloat();
		System.out.print("Enter the height: ");
		float height = input.nextFloat();
		
		float pi = 3.14f;
		
		double area = pi*radius*radius*height;
		System.out.println("The area is: " + area);
	}
}

//17.Volume Of Sphere

package javatpoint;

import java.util.Scanner;

public class Student{
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		//Volume Of Sphere = 4(pi*r*r*r)/3  ;
		
		System.out.print("Enter the radius: ");
		float radius = input.nextFloat();
		
		float pi = 3.14f;
		
		double area = 4*(pi*radius*radius*radius)/3;
		System.out.println("The area is: " + area);
	}
}



//18.Volume Of Pyramid

package javatpoint;

import java.util.Scanner;

public class Student{
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		//Volume Of Pyramid = (length*width*height)/3  ;
		
		System.out.print("Enter the length: ");
		float length = input.nextFloat();
		System.out.print("Enter the width: ");
		float width = input.nextFloat();
		System.out.print("Enter the height: ");
		float height = input.nextFloat();
		
		double area = (length*width*height)/3;
		System.out.println("The area is: " + area);
	}
}


//19.Curved Surface Area Of Cylinder

package javatpoint;

import java.util.Scanner;

public class Student{
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		//Curved Surface Area Of Cylinder = 2*pi*height  ;
		
		System.out.print("Enter the height: ");
		float height = input.nextFloat();
		float pi = 3.14f;
		
		double area = 2*pi*height;
		System.out.println("The area is: " + area);
	}
}

//20.Total Surface Area Of Cube

package javatpoint;

import java.util.Scanner;

public class Student{
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		//Total Surface Area Of Cube = 6*a*a  ;
		
		System.out.print("Enter the area: ");
		float area = input.nextFloat();
		
		double totalArea = 6*area*area;
		System.out.println("The area is: " + totalArea);
	}
}


//21.Fibonacci Series In Java Programs

package javatpoint;

import java.util.Scanner;

public class Student{
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		System.out.print("Enter the number limit: ");
		int timesToPrint = input.nextInt();
		
		int num1 = 0;
		int num2 = 1;
		int i ;
		System.out.print(num1+" "+num2);
		
		for(i=2;i<timesToPrint; i++) {
			int temp = num2+num1;
			System.out.print(" "+temp);
			num1 = num2;
			num2 = temp;
		}
		
		
	}
}


//22.Subtract the Product and Sum of Digits of an Integer

System.out.println("enter the integer: ");
		int number = input.nextInt();
		int temp = number;
		int sum = 0;
		int product = 1;
		
		while(number != 0) { //To find digits of an integer
			int remainder = number%10;
			sum = sum+remainder;
			product = product*remainder;
			number = number/10;
		}
		
		System.out.println("sum of digits of the number, "+temp+" is: "+sum);
		System.out.println("product of digits of the number, "+temp+" is: "+product);
		int difference = product - sum;
		System.out.println("difference of product and sum of digits of, "+temp+" is: "+difference);
		
	}
}
		

//23.Input a number and print all the factors of that number (use loops).

package javatpoint;

import java.util.Scanner;

public class Student{
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		System.out.println("Enter the number: ");
		int number = input.nextInt();
		
		for(int i=1;i<=number;i++) {
			if(number%i == 0) {
				System.out.print(i+" ");
			}
		}
		
		
	}
}


//24.Take integer inputs till the user enters 0 and print the sum of all numbers (HINT: while loop)

package javatpoint;

import java.util.Scanner;

public class Student{
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		int count = 0;
		int sum = 0;
		
		while(true) {
			System.out.println("Enter a number");
			int number = input.nextInt();
			
			if(number>0) {
			sum = number+sum;
			count++;
			
			}
			else{
				System.out.println("The sum is: "+sum);
				System.out.print("Exit");
				break;
				
			}
		
			
		}
		
		
		
		
	}
}


//25.Take integer inputs till the user enters 0 and print the largest number from all.

package javatpoint;

import java.util.Scanner;

public class Student{
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		System.out.println("Enter a number other than 0 to start and enter 0 to terminate: ");
		int number = input.nextInt();
		int a= 0;
		
		while(number!=0) {
			System.out.println("enter a number: ");
			number = input.nextInt();
			
			if(number>a) {
				a=number;
			}
			System.out.println("The largest number is "+a);
			
		}
	}
}









































